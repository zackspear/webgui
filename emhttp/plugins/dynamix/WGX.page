      if (wg=='start') {
        if (ok==0) tstate['wgX']='active'; else $('#toggle-wgX').switchButton({checked:false});
      } else {
        if (ok==0) tstate['wgX']=''; else $('#toggle-wgX').switchButton({checked:true});
      }
