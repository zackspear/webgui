  $('#toggle-wgX').switchButton({labels_placement:'left',off_label:"_(Inactive)_",on_label:"_(Active)_",checked:<?=$check_wgX?>});
  $('#toggle-wgX').change(function(e){
<?if ($this_wgX):?>
